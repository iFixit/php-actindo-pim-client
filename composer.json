{
    "name": "ifixit/php-actindo-pim-client",
    "description": "PHP client for the Actindo PIM",
    "type": "library",
    "repositories": [
       {
          "type": "vcs",
          "url": "https://github.com/sctice/php-code-builder"
       }
    ],
    "require": {
        "swaggest/json-schema": "^0.12.11",
        "swaggest/php-code-builder": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
       "psr-4": {
          "Actindo\\Pim\\": "src"
       }
    },
    "scripts": {
       "post-install-cmd": [
          "Actindo\\Pim\\SchemaBuilder::run"
       ],
       "test": "phpunit --bootstrap vendor/autoload.php tests"
    },
    "authors": [
        {
            "name": "Shawn Tice",
            "email": "shawn@ifixit.com"
        }
    ]
}
